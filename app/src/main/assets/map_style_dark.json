{
  "version": 8,
  "name": "AudioCity Apple-like Dark",
  "sources": {
    "openmaptiles": {
      "type": "vector",
      "url": "https://api.maptiler.com/tiles/v3/tiles.json?key=get_your_own_key"
    }
  },
  "sprite": "https://api.maptiler.com/maps/basic-v2/sprite",
  "glyphs": "https://api.maptiler.com/fonts/{fontstack}/{range}.pbf?key=get_your_own_key",
  "layers": [
    {
      "id": "background",
      "type": "background",
      "paint": {
        "background-color": "#1C1C1E"
      }
    },
    {
      "id": "landuse-residential",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "landuse",
      "filter": ["==", "class", "residential"],
      "paint": {
        "fill-color": "#222224"
      }
    },
    {
      "id": "landuse-commercial",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "landuse",
      "filter": ["==", "class", "commercial"],
      "paint": {
        "fill-color": "#252527"
      }
    },
    {
      "id": "park",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "park",
      "paint": {
        "fill-color": "#1E3D1E",
        "fill-opacity": 0.6
      }
    },
    {
      "id": "landcover-grass",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "landcover",
      "filter": ["==", "class", "grass"],
      "paint": {
        "fill-color": "#1E351E",
        "fill-opacity": 0.5
      }
    },
    {
      "id": "landcover-wood",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "landcover",
      "filter": ["==", "class", "wood"],
      "paint": {
        "fill-color": "#1A2D1A",
        "fill-opacity": 0.4
      }
    },
    {
      "id": "water",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "water",
      "paint": {
        "fill-color": "#1A3040"
      }
    },
    {
      "id": "waterway",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "waterway",
      "paint": {
        "line-color": "#1A3040",
        "line-width": 1.5
      }
    },
    {
      "id": "building",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "building",
      "paint": {
        "fill-color": "#2C2C2E",
        "fill-opacity": 0.8
      }
    },
    {
      "id": "building-outline",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "building",
      "paint": {
        "line-color": "#3A3A3C",
        "line-width": 0.5
      }
    },
    {
      "id": "tunnel-path",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["all", ["==", "brunnel", "tunnel"], ["==", "class", "path"]],
      "paint": {
        "line-color": "#3A3A3C",
        "line-width": 1.5,
        "line-dasharray": [2, 2]
      }
    },
    {
      "id": "road-path",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["all", ["==", "class", "path"], ["!has", "brunnel"]],
      "paint": {
        "line-color": "#3A3A3C",
        "line-width": 1.5
      }
    },
    {
      "id": "road-minor-casing",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["all", ["in", "class", "minor", "service"]],
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "#2C2C2E",
        "line-width": {
          "stops": [[12, 2], [14, 4], [16, 8]]
        }
      }
    },
    {
      "id": "road-minor",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["all", ["in", "class", "minor", "service"]],
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "#3A3A3C",
        "line-width": {
          "stops": [[12, 1], [14, 3], [16, 6]]
        }
      }
    },
    {
      "id": "road-secondary-tertiary-casing",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["in", "class", "secondary", "tertiary"],
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "#2C2C2E",
        "line-width": {
          "stops": [[10, 3], [14, 8], [16, 12]]
        }
      }
    },
    {
      "id": "road-secondary-tertiary",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["in", "class", "secondary", "tertiary"],
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "#48484A",
        "line-width": {
          "stops": [[10, 2], [14, 6], [16, 10]]
        }
      }
    },
    {
      "id": "road-primary-casing",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["==", "class", "primary"],
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "#2C2C2E",
        "line-width": {
          "stops": [[8, 3], [12, 6], [16, 14]]
        }
      }
    },
    {
      "id": "road-primary",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["==", "class", "primary"],
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "#58585A",
        "line-width": {
          "stops": [[8, 2], [12, 4], [16, 12]]
        }
      }
    },
    {
      "id": "road-trunk-casing",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["==", "class", "trunk"],
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "#4A3A1A",
        "line-width": {
          "stops": [[6, 2], [12, 8], [16, 16]]
        }
      }
    },
    {
      "id": "road-trunk",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["==", "class", "trunk"],
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "#6A5A2A",
        "line-width": {
          "stops": [[6, 1], [12, 6], [16, 14]]
        }
      }
    },
    {
      "id": "road-motorway-casing",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["==", "class", "motorway"],
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "#4A3A1A",
        "line-width": {
          "stops": [[5, 2], [10, 6], [16, 18]]
        }
      }
    },
    {
      "id": "road-motorway",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["==", "class", "motorway"],
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "#6A5A2A",
        "line-width": {
          "stops": [[5, 1], [10, 4], [16, 16]]
        }
      }
    },
    {
      "id": "rail",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["==", "class", "rail"],
      "paint": {
        "line-color": "#48484A",
        "line-width": 1.5
      }
    },
    {
      "id": "admin-country",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "boundary",
      "filter": ["==", "admin_level", 2],
      "paint": {
        "line-color": "#58585A",
        "line-width": 1.5,
        "line-dasharray": [3, 2]
      }
    },
    {
      "id": "admin-state",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "boundary",
      "filter": ["==", "admin_level", 4],
      "paint": {
        "line-color": "#48484A",
        "line-width": 1,
        "line-dasharray": [2, 2]
      }
    },
    {
      "id": "place-label-city",
      "type": "symbol",
      "source": "openmaptiles",
      "source-layer": "place",
      "filter": ["==", "class", "city"],
      "layout": {
        "text-field": "{name}",
        "text-font": ["Open Sans Regular"],
        "text-size": {
          "stops": [[6, 12], [10, 16], [14, 20]]
        },
        "text-anchor": "center"
      },
      "paint": {
        "text-color": "#E5E5E7",
        "text-halo-color": "#1C1C1E",
        "text-halo-width": 1.5
      }
    },
    {
      "id": "place-label-town",
      "type": "symbol",
      "source": "openmaptiles",
      "source-layer": "place",
      "filter": ["==", "class", "town"],
      "layout": {
        "text-field": "{name}",
        "text-font": ["Open Sans Regular"],
        "text-size": {
          "stops": [[8, 10], [12, 14]]
        },
        "text-anchor": "center"
      },
      "paint": {
        "text-color": "#C7C7C9",
        "text-halo-color": "#1C1C1E",
        "text-halo-width": 1.2
      }
    },
    {
      "id": "place-label-village",
      "type": "symbol",
      "source": "openmaptiles",
      "source-layer": "place",
      "filter": ["==", "class", "village"],
      "layout": {
        "text-field": "{name}",
        "text-font": ["Open Sans Regular"],
        "text-size": 11,
        "text-anchor": "center"
      },
      "paint": {
        "text-color": "#A1A1A3",
        "text-halo-color": "#1C1C1E",
        "text-halo-width": 1
      }
    },
    {
      "id": "road-label",
      "type": "symbol",
      "source": "openmaptiles",
      "source-layer": "transportation_name",
      "layout": {
        "text-field": "{name}",
        "text-font": ["Open Sans Regular"],
        "text-size": {
          "stops": [[13, 10], [16, 12]]
        },
        "symbol-placement": "line"
      },
      "paint": {
        "text-color": "#98989A",
        "text-halo-color": "#1C1C1E",
        "text-halo-width": 1
      }
    }
  ]
}
