{
  "version": 8,
  "name": "AudioCity Apple-like Light",
  "sources": {
    "openmaptiles": {
      "type": "vector",
      "url": "https://api.maptiler.com/tiles/v3/tiles.json?key=get_your_own_key"
    }
  },
  "sprite": "https://api.maptiler.com/maps/basic-v2/sprite",
  "glyphs": "https://api.maptiler.com/fonts/{fontstack}/{range}.pbf?key=get_your_own_key",
  "layers": [
    {
      "id": "background",
      "type": "background",
      "paint": {
        "background-color": "#F5F5F7"
      }
    },
    {
      "id": "landuse-residential",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "landuse",
      "filter": ["==", "class", "residential"],
      "paint": {
        "fill-color": "#EEEEF0"
      }
    },
    {
      "id": "landuse-commercial",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "landuse",
      "filter": ["==", "class", "commercial"],
      "paint": {
        "fill-color": "#F0F0F2"
      }
    },
    {
      "id": "park",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "park",
      "paint": {
        "fill-color": "#D4E8D4",
        "fill-opacity": 0.7
      }
    },
    {
      "id": "landcover-grass",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "landcover",
      "filter": ["==", "class", "grass"],
      "paint": {
        "fill-color": "#DAE8DA",
        "fill-opacity": 0.6
      }
    },
    {
      "id": "landcover-wood",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "landcover",
      "filter": ["==", "class", "wood"],
      "paint": {
        "fill-color": "#C8DEC8",
        "fill-opacity": 0.5
      }
    },
    {
      "id": "water",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "water",
      "paint": {
        "fill-color": "#B8D4E8"
      }
    },
    {
      "id": "waterway",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "waterway",
      "paint": {
        "line-color": "#B8D4E8",
        "line-width": 1.5
      }
    },
    {
      "id": "building",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "building",
      "paint": {
        "fill-color": "#E0E0E2",
        "fill-opacity": 0.8
      }
    },
    {
      "id": "building-outline",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "building",
      "paint": {
        "line-color": "#D0D0D2",
        "line-width": 0.5
      }
    },
    {
      "id": "tunnel-path",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["all", ["==", "brunnel", "tunnel"], ["==", "class", "path"]],
      "paint": {
        "line-color": "#E8E8EA",
        "line-width": 1.5,
        "line-dasharray": [2, 2]
      }
    },
    {
      "id": "road-path",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["all", ["==", "class", "path"], ["!has", "brunnel"]],
      "paint": {
        "line-color": "#D8D8DA",
        "line-width": 1.5
      }
    },
    {
      "id": "road-minor",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["all", ["in", "class", "minor", "service"]],
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "#FFFFFF",
        "line-width": {
          "stops": [[12, 1], [14, 3], [16, 6]]
        }
      }
    },
    {
      "id": "road-minor-casing",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["all", ["in", "class", "minor", "service"]],
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "#E0E0E2",
        "line-width": {
          "stops": [[12, 2], [14, 4], [16, 8]]
        }
      }
    },
    {
      "id": "road-secondary-tertiary-casing",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["in", "class", "secondary", "tertiary"],
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "#D8D8DA",
        "line-width": {
          "stops": [[10, 3], [14, 8], [16, 12]]
        }
      }
    },
    {
      "id": "road-secondary-tertiary",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["in", "class", "secondary", "tertiary"],
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "#FFFFFF",
        "line-width": {
          "stops": [[10, 2], [14, 6], [16, 10]]
        }
      }
    },
    {
      "id": "road-primary-casing",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["==", "class", "primary"],
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "#D0D0D2",
        "line-width": {
          "stops": [[8, 3], [12, 6], [16, 14]]
        }
      }
    },
    {
      "id": "road-primary",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["==", "class", "primary"],
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "#FFFFFF",
        "line-width": {
          "stops": [[8, 2], [12, 4], [16, 12]]
        }
      }
    },
    {
      "id": "road-trunk-casing",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["==", "class", "trunk"],
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "#E8C87A",
        "line-width": {
          "stops": [[6, 2], [12, 8], [16, 16]]
        }
      }
    },
    {
      "id": "road-trunk",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["==", "class", "trunk"],
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "#F8E8B0",
        "line-width": {
          "stops": [[6, 1], [12, 6], [16, 14]]
        }
      }
    },
    {
      "id": "road-motorway-casing",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["==", "class", "motorway"],
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "#E8C87A",
        "line-width": {
          "stops": [[5, 2], [10, 6], [16, 18]]
        }
      }
    },
    {
      "id": "road-motorway",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["==", "class", "motorway"],
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "#F8E8B0",
        "line-width": {
          "stops": [[5, 1], [10, 4], [16, 16]]
        }
      }
    },
    {
      "id": "rail",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["==", "class", "rail"],
      "paint": {
        "line-color": "#C8C8CA",
        "line-width": 1.5
      }
    },
    {
      "id": "admin-country",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "boundary",
      "filter": ["==", "admin_level", 2],
      "paint": {
        "line-color": "#B0B0B2",
        "line-width": 1.5,
        "line-dasharray": [3, 2]
      }
    },
    {
      "id": "admin-state",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "boundary",
      "filter": ["==", "admin_level", 4],
      "paint": {
        "line-color": "#C8C8CA",
        "line-width": 1,
        "line-dasharray": [2, 2]
      }
    },
    {
      "id": "place-label-city",
      "type": "symbol",
      "source": "openmaptiles",
      "source-layer": "place",
      "filter": ["==", "class", "city"],
      "layout": {
        "text-field": "{name}",
        "text-font": ["Open Sans Regular"],
        "text-size": {
          "stops": [[6, 12], [10, 16], [14, 20]]
        },
        "text-anchor": "center"
      },
      "paint": {
        "text-color": "#444446",
        "text-halo-color": "#FFFFFF",
        "text-halo-width": 1.5
      }
    },
    {
      "id": "place-label-town",
      "type": "symbol",
      "source": "openmaptiles",
      "source-layer": "place",
      "filter": ["==", "class", "town"],
      "layout": {
        "text-field": "{name}",
        "text-font": ["Open Sans Regular"],
        "text-size": {
          "stops": [[8, 10], [12, 14]]
        },
        "text-anchor": "center"
      },
      "paint": {
        "text-color": "#555557",
        "text-halo-color": "#FFFFFF",
        "text-halo-width": 1.2
      }
    },
    {
      "id": "place-label-village",
      "type": "symbol",
      "source": "openmaptiles",
      "source-layer": "place",
      "filter": ["==", "class", "village"],
      "layout": {
        "text-field": "{name}",
        "text-font": ["Open Sans Regular"],
        "text-size": 11,
        "text-anchor": "center"
      },
      "paint": {
        "text-color": "#666668",
        "text-halo-color": "#FFFFFF",
        "text-halo-width": 1
      }
    },
    {
      "id": "road-label",
      "type": "symbol",
      "source": "openmaptiles",
      "source-layer": "transportation_name",
      "layout": {
        "text-field": "{name}",
        "text-font": ["Open Sans Regular"],
        "text-size": {
          "stops": [[13, 10], [16, 12]]
        },
        "symbol-placement": "line"
      },
      "paint": {
        "text-color": "#666668",
        "text-halo-color": "#FFFFFF",
        "text-halo-width": 1
      }
    }
  ]
}
